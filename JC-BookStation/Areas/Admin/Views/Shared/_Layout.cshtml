<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Sistema para controle de Vendas e Comissões">
    <meta name="author" content="JCarvalho TI">
    <link href="~/Images/Logomarca.png" rel="shortcut icon" type="image/x-icon" />
    <title>@ViewBag.Title -> JC-Controler</title>
    
    <!-- Le styles -->
    @Styles.Render("~/Content/css")
    
    <!-- Le javascript -->
    <!-- Placed at the end of the document so the pages load faster -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    
</head>
<body>
    @Html.Partial("_Menu")
    
    <div class="container body-content">
            @RenderBody()
            <hr />
            @Html.Partial("_Rodape")
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
